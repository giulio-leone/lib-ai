{
  "name": "@giulio-leone/lib-ai",
  "version": "0.15.2",
  "type": "module",
  "description": "Servizi AI (config, chat, intent-detection, agent-orchestrator)",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./ai-model.service": {
      "types": "./dist/ai-model.service.d.ts",
      "import": "./dist/ai-model.service.js",
      "require": "./dist/ai-model.service.cjs"
    },
    "./ai-provider-config": {
      "types": "./dist/ai-provider-config.d.ts",
      "import": "./dist/ai-provider-config.js",
      "require": "./dist/ai-provider-config.cjs"
    },
    "./ai-framework-config.service": {
      "types": "./dist/ai-framework-config.service.d.ts",
      "import": "./dist/ai-framework-config.service.js",
      "require": "./dist/ai-framework-config.service.cjs"
    },
    "./ai-provider-config.service": {
      "types": "./dist/ai-provider-config.service.d.ts",
      "import": "./dist/ai-provider-config.service.js",
      "require": "./dist/ai-provider-config.service.cjs"
    },
    "./chat.service": {
      "types": "./dist/chat.service.d.ts",
      "import": "./dist/chat.service.js",
      "require": "./dist/chat.service.cjs"
    },
    "./openrouter-subkey.service": {
      "types": "./dist/openrouter-subkey.service.d.ts",
      "import": "./dist/openrouter-subkey.service.js",
      "require": "./dist/openrouter-subkey.service.cjs"
    },
    "./utils/model-factory": {
      "types": "./dist/utils/model-factory.d.ts",
      "import": "./dist/utils/model-factory.js",
      "require": "./dist/utils/model-factory.cjs"
    },
    "./provider-options-builder": {
      "types": "./dist/provider-options-builder.d.ts",
      "import": "./dist/provider-options-builder.js",
      "require": "./dist/provider-options-builder.cjs"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsup"
  },
  "dependencies": {
    "@giulio-leone/constants": "^0.13.0",
    "@giulio-leone/lib-config": "^0.1.1",
    "@giulio-leone/lib-vercel-admin": "^0.9.0",
    "@giulio-leone/agent-contracts": "workspace:*",
    "@giulio-leone/lib-core": "workspace:*",
    "@giulio-leone/lib-shared": "workspace:*",
    "@giulio-leone/schemas": "workspace:*",
    "@prisma/client": "^7.2.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  }
}
